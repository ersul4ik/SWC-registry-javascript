{
    "name": "swc-registry",
    "version": "1.0.9",
    "description": "Javascript library for accessing SWC-registry content.",
    "license": "MIT",
    "repository": "SmartContractSecurity/SWC-registry-javascript",
    "main": "lib/index.js",
    "types": "src/index.ts",
    "keywords": [
        "swcregistry",
        "npm",
        "package"
    ],
    "files": [
        "lib/index.js",
        "lib/swc-definition.json"
    ],
    "devDependencies": {
        "@types/jest": "^23.3.10",
        "@types/node": "^10.12.10",
        "jest": "^23.6.0",
        "mocha": "^5.2.0",
        "nyc": "^13.1.0",
        "prettier": "^1.15.3",
        "ts-jest": "^23.10.5",
        "ts-node": "^8.0.1",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.17.0",
        "typescript": "^3.2.1"
    },
    "dependencies": {
        "@types/es6-promise": "^3.3.0",
        "chai": "^4.2.0",
        "codecov": "^3.1.0",
        "es6-promise": "^4.2.5",
        "expect.js": "^0.3.1",
        "isomorphic-fetch": "^2.2.1",
        "istanbul": "^0.4.5",
        "load-json-file": "^5.1.0",
        "ts-mockito": "^2.3.1",
        "xmlhttprequest-ts": "^1.0.1"
    },
    "scripts": {
        "build": "tsc && cp src/swc-definition.json lib/ && cp ../README.md ./",
        "test": "nyc mocha -r ts-node/register __tests__/*.test.ts",
        "coverage": "nyc --reporter html --reporter json npm test",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "prepare": "npm run build",
        "prepublishOnly": "npm test",
        "2npm": "npm version patch && npm publish",
        "version": "npm run format"
    },
    "nyc": {
        "extension": [
          ".ts",
          ".tsx"
        ],
        "exclude": [
          "**/*.d.ts"
        ],
        "reporter": [
          "html"
        ],
        "all": true  
    },
    "author": "ersul4ik"
}
