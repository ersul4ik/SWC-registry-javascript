{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "typedef": [true, "call-signature", "property-declaration"],
    "rules": {
        "member-access": false,
        "variable-name": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": ["**/*.json"]
    }
}
